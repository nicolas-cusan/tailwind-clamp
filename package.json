{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "pnpm --filter tailwind-clamp dev",
    "build": "pnpm --filter tailwind-clamp build",
    "docs:dev": "pnpm --filter docs dev",
    "docs:build": "pnpm --filter docs build",
    "docs:generate": "pnpm --filter docs generate",
    "build:all": "pnpm run build && pnpm run docs:build && pnpm run docs:generate",
    "merge:dev": "pnpm --filter tailwind-clamp-merge dev",
    "merge:build": "pnpm --filter tailwind-clamp-merge build",
    "merge:test": "pnpm --filter tailwind-clamp-merge test",
    "release": "./scripts/release.sh"
  },
  "devDependencies": {
    "np": "^11.0.2",
    "prettier": "^3.8.1"
  }
}
