{
  "name": "tailwind-clamp",
  "version": "3.0.0",
  "description": "Tailwind CSS plugin to use CSS clamp in your projects",
  "main": "dist/index.umd.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "/dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:docs": "vite build --config vite.config.docs.js",
    "build:all": "npm run build && npm run build:docs",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicolas-cusan/tailwind-clamp.git"
  },
  "keywords": [
    "tailwind",
    "css",
    "clamp"
  ],
  "author": "Nicolas Cusan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicolas-cusan/tailwind-clamp/issues"
  },
  "homepage": "https://github.com/nicolas-cusan/tailwind-clamp#readme",
  "dependencies": {
    "tailwindcss": "^3.4.14"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "vite": "^5.4.10"
  }
}
